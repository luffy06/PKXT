<%- include('components/header') -%>
    <!-- page集合的容器，里面放多个平行的.page，其他.page作为内联页面由路由控制展示 -->
    <div class="page-group">
        <!-- 单个page ,第一个.page默认被展示-->
        <div class="page" id='assess'>
            <!-- 标题栏 -->
            <header class="bar bar-nav">
                <a class="icon icon-me pull-left open-panel"></a>
                <a class="icon icon-code pull-right"></a>
                <h1 class="title">标题</h1>
            </header>
            <!-- 这里是页面内容区 -->
            <div class="content">
                <div class="row" >
                    <div class="col-50">
                        <span>得分：</span>
                        <span id='score'>0</span>
                    </div>
                    <div class="col-50">
                        <span>耗时：</span>
                        <span id='time'>0</span>
                    </div>
                </div>
                <div class="content-block">
                    <div class="card">
                        <div class="card-header" id='questionNumber'>问题1</div>
                        <div class="card-content">
                            <div class="card-content-inner" id='questionContent'>问题详细内容</div>
                        </div>
                        <div class="card-footer" id="questionChoices">
                            <div class="list-block">
                                <ul>
                                    <li>
                                        <p><a href="" class="button  button-primary">A选项内容</a>
                                        </p>
                                    </li>
                                    <li>
                                        <p><a href="" class="button  button-primary">B选项内容</a>
                                        </p>
                                    </li>
                                    <li>
                                        <p><a href="" class="button  button-primary">C选项内容</a>
                                        </p>
                                    </li>
                                    <li>
                                        <p><a href="" class="button  button-primary">D选项内容</a>
                                        </p>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- 其他的单个page内联页（如果有） -->
    </div>
    <%- include('components/sidebar') -%>
    <%- include('components/footer') -%>
